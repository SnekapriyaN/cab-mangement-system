<style>
    body {
        margin: 0;
        padding: 0;
    }

    .container {
        display: flex;
        
    }

    .box1 {
  
        margin-left:10px;
        margin-top: 170px;

    } 
    .center{
      margin-top: 250px;
      margin-left: 90px;
      }
      .btn{
        margin-left: 120px;
      }
   
* {box-sizing: border-box;}


.topnav {
  overflow: hidden;
  background-color: #e9e9e9;
}
.topnav a {
  float: left;
  display: block;
  color: black;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
}


.topnav a:hover {
  background-color: #ddd;
  color: black;
}


